gboolean on_smartstreetlight_main_notebook_switch_page (GtkNotebook *notebook,
                                               GtkWidget   *page,
                                               guint        page_num,
                                               gpointer     user_data)
{
    DEBUG_1();
    DEBUG("=============== page_num: %d", page_num);

    /*
    enum SPL_NOTEBOOK_PAGE_Enum
    {
        SPL_NOTEBOOK_DCU_PAGE           = 0,
        SPL_NOTEBOOK_PROGRAM_PAGE       = 1,
        SPL_NOTEBOOK_USER_LOG_PAGE      = 2,
        SPL_NOTEBOOK_SERVER_LOG_PAGE    = 3,
        SPL_NOTEBOOK_USER_MANAGE_PAGE   = 4,
        SPL_NOTEBOOK_ZONE_MANAGE_PAGE   = 5,
        SPL_NOTEBOOK_PAGE_NUMBER
    };
    */


    SPL_NOTEBOOK_PAGE_Enum page_index = (SPL_NOTEBOOK_PAGE_Enum) page_num;

    switch(page_index)
    {
    case SPL_NOTEBOOK_DCU_PAGE:
        if(is_changed_map_parent_by_admin_zone)
        {
            osm_gps_map_change_parent("admin_zone_map_box", "map_box");
            osm_osm_gps_map_show_lights(gtk_combo_box_get_active(GTK_COMBO_BOX(cbb_light_display)),lastSelectedDcu);
            is_changed_map_parent_by_admin_zone = FALSE;
        }
        break;
    case SPL_NOTEBOOK_PROGRAM_PAGE:
        break;
    case SPL_NOTEBOOK_USER_LOG_PAGE:
        smartstreetlight_user_log_show_all();
        break;
    case SPL_NOTEBOOK_SERVER_LOG_PAGE:
        smartstreetlight_server_log_show_all();
        break;
    case SPL_NOTEBOOK_USER_MANAGE_PAGE:
        break;
    case SPL_NOTEBOOK_ZONE_MANAGE_PAGE:
        if(spl_admin_zone_show_tab_on_notebook())
        {
            is_changed_map_parent_by_admin_zone = TRUE;
            spl_admin_zone_run();
        }
        break;
    default:
        break;
    };

    return FALSE;
}


