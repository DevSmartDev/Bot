void smartstreetlight_dcu_popup_menu_clicked(GtkWidget *popup_menu_item, gpointer userdata)
{
    SPL_DCU_POPUP_MENU_Enum type = (SPL_DCU_POPUP_MENU_Enum)GPOINTER_TO_INT(userdata);
    switch(type)
    {
    case SPL_DCU_M_ADD:
        gboolean add_result;
        do
        {
            add_result = smartstreetlight_dcu_add_new();
            add_result = TRUE;
        }
        while(!add_result);
        break;
    case SPL_DCU_M_EDIT:
        smartstreetlight_dcu_edit(lastSelectedDcu);
        break;
    case SPL_DCU_M_LOG:
        smartstreetlight_dcu_view_log(lastSelectedDcu);
        //smartstreetlight_dcu_popup_menu_log_clicked();
        break;
    case SPL_DCU_M_SCHEDULE:
        spl_dcu_control_dialog_show(DIALOG_CONTROL_IS_SCHEDULE, lastSelectedDcu);
        break;
    case SPL_DCU_M_REMOVE:
        {
            switch(spl_utils_message_box(GTK_WINDOW(main_widgets->add_dcu_dialog),
                                        GTK_MESSAGE_WARNING,
                                        GTK_BUTTONS_OK_CANCEL,
                                        "Bạn có chắc muốn xóa trạm này, tất cả các đèn thuộc trạm này sẽ bị xóa"))
            {
                case GTK_RESPONSE_OK:
                {
                    if(lastSelectedDcu)
                    {
                        pSplDcu dcuRemove = lastSelectedDcu;
                        if(smartstreetlight_remove_dcu_execute(dcuRemove->priv->id) == TRUE)
                        {
                            //clear this dcu on treeview
                            if(gtk_tree_store_remove(main_view_widgets->dcu_store, &dcuRemove->priv->iter))
                            {
                                spl_delete_all_lcu_on_dcu(dcuRemove); //delete all his child
                                REMOVE_LIST_DCU(dcuRemove);
                            }
                        }
                    }
                    DEBUG("OK\n");
                }
                break;
                case GTK_RESPONSE_CANCEL:
                    DEBUG("CANCEL\n");
                default:
                    break;
            }
        }
        //smartstreetlight_dcu_popup_menu_remove_clicked();
        break;
    case SPL_DCU_M_REMOVE_ALL:
        //smartstreetlight_dcu_popup_menu_remove_all_clicked();
        break;
    case SPL_DCU_M_CONTROL:
        spl_dcu_control_dialog_show(DIALOG_CONTROL_IS_ON_OFF, lastSelectedDcu);
        //spl_dcu_control_dialog_show(DIALOG_CONTROL_IS_SETTING);
        break;
    default:
        break;
    }
}

void spl_lcu_popup_menu_clicked(GtkWidget *w, gpointer usr_data)
{
    gint type = GPOINTER_TO_INT(usr_data);
    switch(type)
    {
        case SPL_LCU_M_ADD:
            spl_light_add_new();
            break;
        case SPL_LCU_M_ADD_FROM_FILE:
            //spl_light_add_new_from_file();
            break;
        case SPL_LCU_M_EDIT:
            spl_light_edit(lastSelectedLcu);
            break;
        case SPL_LCU_M_REMOVE:
            if(!spl_light_remove(lastSelectedLcu))
            {
                //spl_utils_message_box(GTK_WINDOW(main_widgets->main_window), GTK_MESSAGE_ERROR, GTK_BUTTONS_OK,
                 //                     "Thao tác không thực hiện được");
            }
            break;
        case SPL_LCU_M_REMOVE_ALL:
            //if(xoa all lcu on dcuID)
            {
                lastSelectedLcu = NULL;
                osm_osm_gps_map_remove_dcu(lastSelectedDcu);
                spl_delete_all_lcu_on_dcu(lastSelectedDcu);
            }
            break;
        case SPL_LCU_M_CONTROL:
            spl_lcu_popup_menu_control(lastSelectedLcu);
            break;
        default:
            break;
    }
}

