    DEBUG_1();
    const gchar *dcu_id = gtk_label_get_text(GTK_LABEL(SPL_OBJECT("dcu_info_id")));
    const gint dcu_mode = gtk_combo_box_get_active(GTK_COMBO_BOX(SPL_OBJECT("dcu_info_mode_combobox")));
    const gchar *dcu_program_name = "aaa"; //smartstreetlight_program_get_active_name();

    DEBUG("\n ================ zone_id: %s", gtk_label_get_text(GTK_LABEL(SPL_OBJECT("dcu_info_zone_id"))));

    const gint zone_id = (gint)g_ascii_strtoll(gtk_label_get_text(GTK_LABEL(SPL_OBJECT("dcu_info_zone_id"))), NULL, 0);

    if(smartstreetlight_dcu_schedule_update_execute(smartstreetlight_username, smartstreetlight_session_id,
                                             dcu_id, dcu_mode, dcu_program_name))
    {
        DEBUG_1();
        if(smartstreetlight_dcu_set_program(zone_id, g_strdup(dcu_id), g_strdup(dcu_program_name)))
        {
            DEBUG_1();
            smartstreetlight_hide_button_combobox();
        }
        DEBUG_1();

    }
    else
    {
        DEBUG_1();
    }
    DEBUG_1();
}


