        if (smartstreetlight_dcu_list_store_init())
        {
            if (smartstreetlight_dcu_tree_store_init())
            {
                if (smartstreetlight_dcu_popup_menu_init())
                {
                    if (smartstreetlight_dcu_callback_init())
