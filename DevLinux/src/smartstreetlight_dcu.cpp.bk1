                            -1);

    smartstreetlight_dcu_show_specific_info(dcu_id, dcu_phone, dcu_gps, dcu_address, dcu_note,
                                            dcu_mode, dcu_status, NULL, NULL,
                                            dcu_zone_id, dcu_zone_name);

    g_free(dcu_status);
    g_free(dcu_zone_name);


