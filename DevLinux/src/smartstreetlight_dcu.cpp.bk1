static void smartstreetlight_dcu_popup_menu_clicked(GtkWidget *popup_menu_item, gpointer userdata)
{
    SPL_DCU_POPUP_MENU_Enum type = (SPL_DCU_POPUP_MENU_Enum)GPOINTER_TO_INT(userdata);
    switch(type)
    {
    case SPL_DCU_M_ADD:
        gboolean add_result;
        do
        {
            add_result = smartstreetlight_dcu_popup_menu_add_clicked();
        }
        while(!add_result);
        break;
    case SPL_DCU_M_EDIT:
        smartstreetlight_dcu_popup_menu_edit_clicked();
        break;
    case SPL_DCU_M_LOG:
        smartstreetlight_dcu_popup_menu_log_clicked();
        break;
    case SPL_DCU_M_SCHEDULE:
        smartstreetlight_dcu_popup_menu_schedule_clicked();
        break;
    case SPL_DCU_M_REMOVE:
        smartstreetlight_dcu_popup_menu_remove_clicked();
        break;
    case SPL_DCU_M_REMOVE_ALL:
        smartstreetlight_dcu_popup_menu_remove_all_clicked();
        break;
    default:
        break;
    }
}

static void smartstreetlight_dcu_popup_menu_widget_init(void)
{
    GtkWidget *separator_item;
    dcu_info_popup_menu->M_Popup = gtk_menu_new();
    SPL_MENU_CSS(dcu_info_popup_menu->M_Popup);

    dcu_info_popup_menu->M_Add = gtk_menu_item_new_with_label("Thêm mới");
    gtk_menu_shell_append(GTK_MENU_SHELL(dcu_info_popup_menu->M_Popup),dcu_info_popup_menu->M_Add);

    separator_item = gtk_separator_menu_item_new();
    gtk_menu_shell_append(GTK_MENU_SHELL(dcu_info_popup_menu->M_Popup), separator_item);

    dcu_info_popup_menu->M_Edit = gtk_menu_item_new_with_label("Chỉnh sửa");
    gtk_menu_shell_append(GTK_MENU_SHELL(dcu_info_popup_menu->M_Popup),dcu_info_popup_menu->M_Edit);

    separator_item = gtk_separator_menu_item_new();
