gchar * smartstreetlight_get_mode_name(SPL_DCU_MODE_Enum index)
{
    switch(index)
    {
    case SPL_DCU_WirelessMode:
        return g_strdup(SPL_DCU_WIRELESS_MODE);
    case SPL_DCU_LineMode:
        return g_strdup(SPL_DCU_LINE_MODE);
    case SPL_DCU_UnknownMode:
        return g_strdup(SPL_DCU_UNKNOWN_MODE);
    default:
        return NULL;
    }
}

gchar * smartstreetlight_get_status_name(SPL_DCU_STATUS_Enum index)
{
    switch(index)
    {
    case SPL_DCU_Active:
        return g_strdup(SPL_STATUS_ACTIVE);
    case SPL_DCU_Unactive:
        return g_strdup(SPL_STATUS_INACTIVE);
    default:
        return NULL;
    }
}

static void smartstreetlight_dcu_specific_gui_update(SPL_DCU_MODE_Enum mode)
{
    GtkWidget *lcu_info_box = GTK_WIDGET(gtk_builder_get_object (smartstreetlight_builder, "lcu_info_box" ));
    GtkWidget *line_info_box= GTK_WIDGET(gtk_builder_get_object (smartstreetlight_builder, "line_info_box"));

    switch(mode)
    {
    case SPL_DCU_WirelessMode:
        gtk_widget_show_all(lcu_info_box);
        gtk_widget_hide(line_info_box);
        gtk_widget_set_visible(GTK_WIDGET(SPL_OBJECT("dcu_program_edit_btn")), TRUE);
        break;
    case SPL_DCU_LineMode:
        gtk_widget_hide(lcu_info_box);
        gtk_widget_show_all(line_info_box);
        gtk_widget_set_visible(GTK_WIDGET(SPL_OBJECT("dcu_program_edit_btn")), TRUE);
        break;
    case SPL_DCU_UnknownMode:
        gtk_widget_hide(lcu_info_box);
        gtk_widget_hide(line_info_box);
        break;
    case SPL_DCU_Init:
    default:
        gtk_widget_hide(lcu_info_box);
        gtk_widget_hide(line_info_box);
        gtk_widget_set_visible(GTK_WIDGET(SPL_OBJECT("dcu_program_edit_btn")), FALSE);
        break;
    }
}


